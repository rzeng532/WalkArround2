<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/bgcor11" />
            <stroke android:color="@color/linecor1" android:width="0.5dp" />
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/bgcor12" />
            <stroke android:color="@color/linecor1" android:width="0.5dp" />
            <corners android:radius="2dp"/>
        </shape>
    </item>
</selector>